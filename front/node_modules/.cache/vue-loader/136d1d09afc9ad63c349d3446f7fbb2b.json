CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Matches.vue","sourceRoot":"src/views/dashboard/matches","sourcesContent":["<template>\r\n  <v-container>\r\n    <div class=\"container\">\r\n      <h1 v-if=\"currentGroupIndex==0\">Group Stage</h1>\r\n      <h1 v-else>Knockout Stage</h1>\r\n      <button\r\n        v-show=\"choice==false\"\r\n        style=\"text-align: center;\"\r\n        class=\"btn btn-success\"\r\n        v-on:click=\"choice=true; loading=true; initializeGroup()\"\r\n      >Generate Group Matches</button>\r\n      <button\r\n        v-show=\"choice==false\"\r\n        style=\"text-align: center;\"\r\n        class=\"btn btn-success\"\r\n        v-on:click=\"choice=true; getGroups();\"\r\n      >Continue With Latest Matchups</button>\r\n    </div>\r\n    <div v-if=\"choice\" class=\"container\">\r\n      <table v-if=\"currentGroupIndex==0\" class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th scope=\"col\">Team</th>\r\n            <th scope=\"col\">P</th>\r\n            <th scope=\"col\">W</th>\r\n            <th scope=\"col\">D</th>\r\n            <th scope=\"col\">L</th>\r\n            <th scope=\"col\">GF</th>\r\n            <th scope=\"col\">GA</th>\r\n            <th scope=\"col\">GD</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(team, i) in table\" :key=\"i\">\r\n            <th scope=\"row\">\r\n              <img style=\"width:30px;\" :src=\"team[0].image_link\" />\r\n              ${getTeamParticipant(team[0].participant).name}\r\n            </th>\r\n            <td>${team[1].P}</td>\r\n            <td>${team[1].W}</td>\r\n            <td>${team[1].D}</td>\r\n            <td>${team[1].L}</td>\r\n            <td>${team[1].GF}</td>\r\n            <td>${team[1].GA}</td>\r\n            <td>${team[1].GD}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <table v-if=\"choice&&groups.length>0\" class=\"table table-striped\">\r\n        <thead class=\"thead-dark\">\r\n          <tr>\r\n            <th scope=\"col\">Match</th>\r\n            <th scope=\"col\">Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(match, i) in currentGroup.matches\" :key=\"i\">\r\n            <th v-if=\"match.goals_team_1 == null && match.goals_team_2 == null\" scope=\"row\">\r\n              ${getTeamParticipant(getTeamById(match.team_1).participant).name}\r\n              <img\r\n                style=\"width:30px;\"\r\n                :src=\"getTeamById(match.team_1).image_link\"\r\n              />\r\n              X\r\n              <img style=\"width:30px;\" :src=\"getTeamById(match.team_2).image_link\" />\r\n              ${getTeamParticipant(getTeamById(match.team_2).participant).name}\r\n            </th>\r\n            <th v-else scope=\"row\">\r\n              ${getTeamParticipant(getTeamById(match.team_1).participant).name}\r\n              <img\r\n                style=\"width:30px;\"\r\n                :src=\"getTeamById(match.team_1).image_link\"\r\n              />\r\n              ${match.goals_team_1} X\r\n              ${match.goals_team_2}\r\n              <img\r\n                style=\"width:30px;\"\r\n                :src=\"getTeamById(match.team_2).image_link\"\r\n              />\r\n              ${getTeamParticipant(getTeamById(match.team_2).participant).name}\r\n            </th>\r\n            <td>\r\n              <button class=\"btn btn-success\" v-on:click=\"getMatch(match.id)\">Register Score</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <button\r\n        v-if=\"!currentGroupIndex==0\"\r\n        style=\"float:left;\"\r\n        class=\"btn btn-primary\"\r\n        v-on:click=\"currentGroupIndex--; currentGroup=groups[currentGroupIndex]; final=false;\"\r\n      >Previous Stage</button>\r\n      <a\r\n        v-if=\"currentGroupIndex==0\"\r\n        style=\"float: left;\"\r\n        class=\"btn btn-primary\"\r\n        :href=\"'/webapp'\"\r\n      >Go Back to Draft</a>\r\n      <button\r\n        v-if=\"currentGroupIndex==0\"\r\n        style=\"float:right;\"\r\n        class=\"btn btn-success\"\r\n        v-on:click=\"knockoutStageButtonClick();\"\r\n      >Knockout Stage</button>\r\n      <button\r\n        v-if=\"!currentGroupIndex==0&&!final\"\r\n        style=\"float:right;\"\r\n        class=\"btn btn-success\"\r\n        v-on:click=\"nextKnockoutStageButtonClick();\"\r\n      >Next Knockout</button>\r\n      <button\r\n        v-if=\"final\"\r\n        style=\"float:right;\"\r\n        v-on:click=\"generateChampzFile()\"\r\n        class=\"btn btn-outline-warning\"\r\n      >Generate Champz File</button>\r\n    </div>\r\n    <br />\r\n    <br />\r\n    <br />\r\n    <br />\r\n    <!-- Register Score Modal -->\r\n    <div\r\n      v-show=\"choice\"\r\n      class=\"modal fade\"\r\n      id=\"registerScoreModal\"\r\n      tabindex=\"-1\"\r\n      role=\"dialog\"\r\n      aria-labelledby=\"exampleModalLongTitle\"\r\n      aria-hidden=\"true\"\r\n    >\r\n      <div class=\"modal-dialog\" role=\"document\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"exampleModalLongTitle\">REGISTER SCORE</h5>\r\n            <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\">\r\n              <span aria-hidden=\"true\">&times;</span>\r\n            </button>\r\n          </div>\r\n          <form v-on:submit.prevent=\"registerScore()\">\r\n            <div class=\"modal-body\">\r\n              <div class=\"container\">\r\n                <div class=\"row\">\r\n                  <a>${getTeamParticipant(getTeamById(currentMatch.team_1).participant).name}</a>\r\n                  <div class=\"col-sm-3\">\r\n                    <input\r\n                      type=\"number\"\r\n                      min=\"0\"\r\n                      class=\"form-control\"\r\n                      id=\"goals_team_1\"\r\n                      placeholder\r\n                      v-model=\"currentMatch.goals_team_1\"\r\n                      required=\"required\"\r\n                    />\r\n                  </div>\r\n                  <a>X</a>\r\n                  <div class=\"col-sm-3\">\r\n                    <input\r\n                      type=\"number\"\r\n                      min=\"0\"\r\n                      class=\"form-control\"\r\n                      id=\"goals_team_2\"\r\n                      placeholder\r\n                      v-model=\"currentMatch.goals_team_2\"\r\n                      required=\"required\"\r\n                    />\r\n                  </div>\r\n                  <a>${getTeamParticipant(getTeamById(currentMatch.team_2).participant).name}</a>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"modal-footer\">\r\n              <button type=\"button\" class=\"btn btn-secondary m-progress\" data-dismiss=\"modal\">Close</button>\r\n              <button type=\"submit\" class=\"btn btn-primary\">Save changes</button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n      <div class=\"loading\" v-if=\"loading===true\">Loading&#8230;</div>\r\n    </div>\r\n    <!-- End Register Score modal -->\r\n    <div class=\"loading\" v-if=\"loading===true\">Loading&#8230;</div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport Service from \"@/services/Service\";\r\nexport default {\r\n  name: \"Matches\",\r\n\r\n  data: {\r\n    service: new Service(),\r\n    loading: true,\r\n    choice: false,\r\n    final: false,\r\n    participants: [],\r\n    groupMatches: [],\r\n    plTeams: [],\r\n    groups: [],\r\n    matches: [],\r\n    table: [],\r\n    currentMatch: {},\r\n    currentGroupIndex: 0,\r\n    currentGroup: {},\r\n  },\r\n  mounted: function () {\r\n    this.getParticipants();\r\n    this.getPLTeams();\r\n    // this.initializeGroup();\r\n  },\r\n  methods: {\r\n    getPLTeams: function () {\r\n      this.service\r\n        .getRequest(\"/api/team?league=72\")\r\n        .then((response) => {\r\n          this.plTeams = response.data;\r\n          this.loading = false;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    getParticipants: function () {\r\n      this.service\r\n        .getRequest(\"/api/participant/\")\r\n        .then((response) => {\r\n          this.participants = response.data;\r\n          this.loading = false;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    getTeamParticipant: function (id) {\r\n      participant = {};\r\n      this.participants.forEach((element) => {\r\n        if (element.id === id) {\r\n          participant = element;\r\n          return participant;\r\n        }\r\n      });\r\n      return participant;\r\n    },\r\n    getTeamById: function (id) {\r\n      team = {};\r\n      this.plTeams.forEach((element) => {\r\n        if (element.id === id) {\r\n          team = element;\r\n          return team;\r\n        }\r\n      });\r\n      return team;\r\n    },\r\n    getMatch: function (id) {\r\n      this.loading = true;\r\n      url = \"/api/match/\" + id + \"/\";\r\n      this.service\r\n        .get(url)\r\n        .then((response) => {\r\n          this.currentMatch = response.data;\r\n          $(\"#registerScoreModal\").modal(\"show\");\r\n          this.loading = false;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    deleteGroups: function () {\r\n      var i;\r\n      groupsToDelete = this.groups.slice(this.currentGroupIndex + 1);\r\n      for (i = 0; i < groupsToDelete.length; i++) {\r\n        this.deleteGroup(groupsToDelete[i].id);\r\n      }\r\n    },\r\n    deleteGroup: function (id) {\r\n      this.loading = true;\r\n      url = \"/api/group/\" + id + \"/\";\r\n      this.service\r\n        .delete(url)\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.getGroups();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    registerScore: function () {\r\n      this.loading = true;\r\n      var self = this;\r\n      url = \"/api/match/\" + this.currentMatch.id + \"/\";\r\n      this.service\r\n        .patch(url, this.currentMatch)\r\n        .then((response) => {\r\n          this.loading = false;\r\n          this.currentMatch = response.data;\r\n          if (this.currentGroupIndex + 1 < this.groups.length) {\r\n            this.deleteGroups();\r\n          } else {\r\n            this.getGroups();\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    getGroups: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .getRequest(\"/api/group/\")\r\n        .then((response) => {\r\n          this.groups = response.data;\r\n          console.log(\"groups\", response.data);\r\n          this.groups.forEach((element) => {\r\n            element.matches = [];\r\n          });\r\n          this.getMatches();\r\n          this.getGroupTable();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    getGroupTable: function () {\r\n      this.service\r\n        .getRequest(\"/api/table/\" + this.groups[0].id)\r\n        .then((response) => {\r\n          this.table = response.data;\r\n          this.loading = false;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    initializeGroup: function () {\r\n      this.service\r\n        .postRequest(\"/api/start_group/\")\r\n        .then((response) => {\r\n          this.getGroups();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    knockoutStageButtonClick: function () {\r\n      console.log(this.currentGroupIndex, this.groups);\r\n      if (this.groups.length > 1) {\r\n        this.currentGroupIndex += 1;\r\n        this.currentGroup = this.groups[this.currentGroupIndex];\r\n      } else {\r\n        this.loading = true;\r\n        this.generateWildcard();\r\n      }\r\n    },\r\n    nextKnockoutStageButtonClick: function () {\r\n      if (this.currentGroupIndex + 1 < this.groups.length) {\r\n        this.currentGroupIndex += 1;\r\n        this.currentGroup = this.groups[this.currentGroupIndex];\r\n        if (this.currentGroup.matches.length == 1) {\r\n          this.final = true;\r\n        }\r\n      } else {\r\n        console.log(2, this.currentGroupIndex);\r\n        this.loading = true;\r\n        if (this.currentGroupIndex == 1) {\r\n          this.generateSemis();\r\n        } else {\r\n          this.generateFinal();\r\n        }\r\n      }\r\n    },\r\n    generateFirstKnockout: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .postRequest(\"/api/generate_1st_knockout/\")\r\n        .then((response) => {\r\n          this.currentGroupIndex += 1;\r\n          this.getGroups();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    generateWildcard: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .postRequest(\"/api/generate_wildcards/\")\r\n        .then((response) => {\r\n          this.currentGroupIndex += 1;\r\n          this.getGroups();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    generateSemis: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .postRequest(\"/api/generate_semis/\")\r\n        .then((response) => {\r\n          this.currentGroupIndex += 1;\r\n          this.getGroups();\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    generateFinal: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .postRequest(\"/api/generate_final/\" + this.groups[this.currentGroupIndex].id)\r\n        .then((response) => {\r\n          this.getGroups();\r\n          this.currentGroupIndex += 1;\r\n          this.final = true;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    generateChampzFile: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .postRequest(\"/api/end_champz/\")\r\n        .then((response) => {\r\n          this.loading = false;\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n    matchToGroup: function (match) {\r\n      this.groups.forEach((element) => {\r\n        if (match.group == element.id) {\r\n          element.matches.push(match);\r\n          return;\r\n        }\r\n      });\r\n    },\r\n    getMatches: function () {\r\n      this.loading = true;\r\n      this.service\r\n        .getRequest(\"/api/match/\")\r\n        .then((response) => {\r\n          this.matches = response.data;\r\n          this.matches.forEach((element) => {\r\n            this.matchToGroup(element);\r\n          });\r\n          this.currentGroup = this.groups[this.currentGroupIndex];\r\n          if (this.currentGroup.matches.length == 1) {\r\n            this.final = true;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          this.loading = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n"]}]}